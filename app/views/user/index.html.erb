<div class="container2">
  <div class="row">
    <h1>User <%= @user.email %></h1>
  </div>

  <div class="row">
    <div class="col-xs-6">
      <div class="row">
        <%= link_to 'Subscribe for $5', '', class: 'btn btn-success' %>
      </div>
    </div>
    <div class="col-xs-6">
      <ul>
        <% @results.each do |result| -%>
        <li>
          <%= result.score %><br>
          <%= image_tag result.image %><br>
          <%= result.notes %>
        </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
